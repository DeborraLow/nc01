{% macro box(creator, type, title, img) %}
    <li class="project-overview">
        <div class="project-creator">{{ creator }}</div>
        <div class="project-type">{{ type }}</div>
        <h2 class="project-title">{{ title }}</h2>
        <img class="project-img" src="{{ img }}" alt="{{ title }}">
    </li>
{% endmacro %}
